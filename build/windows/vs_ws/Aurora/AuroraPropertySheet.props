<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <Aurora_HOME>D:\work\Aurora</Aurora_HOME>
    <Aurora_BUILD>$(Aurora_HOME)\build\windows\vs_build</Aurora_BUILD>
    <Aurora_LIB_OUT>$(Aurora_BUILD)\$(PlatformTarget)\lib\$(Configuration)</Aurora_LIB_OUT>
    <Aurora_BIN_OUT>$(Aurora_BUILD)\$(PlatformTarget)\bin\$(Configuration)</Aurora_BIN_OUT>
    <Aurora_IMP>$(Aurora_HOME)\aurora</Aurora_IMP>
    <Aurora_BIN>$(Aurora_HOME)\bin\windows</Aurora_BIN>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="Aurora_HOME">
      <Value>$(Aurora_HOME)</Value>
    </BuildMacro>
    <BuildMacro Include="Aurora_BUILD">
      <Value>$(Aurora_BUILD)</Value>
    </BuildMacro>
    <BuildMacro Include="Aurora_LIB_OUT">
      <Value>$(Aurora_LIB_OUT)</Value>
    </BuildMacro>
    <BuildMacro Include="Aurora_BIN_OUT">
      <Value>$(Aurora_BIN_OUT)</Value>
    </BuildMacro>
    <BuildMacro Include="Aurora_IMP">
      <Value>$(Aurora_IMP)</Value>
    </BuildMacro>
    <BuildMacro Include="Aurora_BIN">
      <Value>$(Aurora_BIN)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>