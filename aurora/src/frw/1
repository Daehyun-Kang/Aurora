/* include header files */
#include "aurora.h"
#include "frw/az_frw.h"

static az_fsm_state_t az_frw_fsm_handleOnIdle(void *ctx, az_event_t *evt);
static az_fsm_state_t az_frw_fsm_handleOnInit(void *ctx, az_event_t *evt);
static az_fsm_state_t az_frw_fsm_handleOnCnfg(void *ctx, az_event_t *evt);
static az_fsm_state_t az_frw_fsm_handleOnSync(void *ctx, az_event_t *evt);
static az_fsm_state_t az_frw_fsm_handleOnIerr(void *ctx, az_event_t *evt);
static az_fsm_state_t az_frw_fsm_handleOnNorm(void *ctx, az_event_t *evt);
static az_fsm_state_t az_frw_fsm_handleOnDown(void *ctx, az_event_t *evt);

/* declare global variables */
#define AZ_FRW_FSM_VECTOR(state, handle) {AZ_FRW_STATE_##state, az_frw_fsm_##handle}
az_fsm_vector_t az_frw_fsm_table[] = {
  AZ_FRW_FSM_VECTOR(IDLE, handleOnIdle),
  AZ_FRW_FSM_VECTOR(INIT, handleOnInit),
  AZ_FRW_FSM_VECTOR(CNFG, handleOnCnfg),
  AZ_FRW_FSM_VECTOR(SYNC, handleOnSync),
  AZ_FRW_FSM_VECTOR(IERR, handleOnIerr),
  AZ_FRW_FSM_VECTOR(NORM, handleOnNorm),
  AZ_FRW_FSM_VECTOR(DOWN, handleOnDown),
  {0, NULL}
};

az_fsm_t  az_frw_fsm = 
  AZ_FSM_INIT_SEQ("frw_fsm", AZ_FRW_STATE_IDLE, az_frw_fsm_table, NULL);

