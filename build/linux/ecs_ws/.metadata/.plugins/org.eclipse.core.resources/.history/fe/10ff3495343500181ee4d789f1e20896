# Add inputs and outputs from these tool invocations to the build variables 

# All Target
all: auros

# Tool invocations
auros: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: GCC C++ Linker'
	g++ -L"/home/dhkang/work/Aurora/build/linux/ecs_build/x86_64/Release/lib" -static -o "auros" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '
	$(MAKE) --no-print-directory post-build

# Other Targets
clean:
	-$(RM) $(CC_DEPS)$(C++_DEPS)$(EXECUTABLES)$(C_UPPER_DEPS)$(CXX_DEPS)$(OBJS)$(CPP_DEPS)$(C_DEPS) auros
	-@echo ' '

post-build:
	-@echo 'copy exeutable to output location'
	-cp auros /home/dhkang/work/Aurora/build/linux/ecs_build/x86_64/Release/bin/; cp auros /home/dhkang/work/Aurora/bin/linux/
	-@echo ' '

.PHONY: all clean dependents
.SECONDARY: post-build

